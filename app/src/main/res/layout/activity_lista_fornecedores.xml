<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.FornecedoresActivity">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_todos_fornecedores"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_fornecedor"
        android:layout_width="62dp"
        android:layout_height="67dp"
        android:clickable="true"
        android:onClick="adicionarFornecedor"
        app:layout_constraintBottom_toBottomOf="@+id/rv_todos_fornecedores"
        app:layout_constraintEnd_toEndOf="@+id/rv_todos_fornecedores"
        app:layout_constraintHorizontal_bias="0.915"
        app:layout_constraintStart_toStartOf="@+id/rv_todos_fornecedores"
        app:layout_constraintTop_toTopOf="@+id/rv_todos_fornecedores"
        app:layout_constraintVertical_bias="0.969"
        app:srcCompat="@android:drawable/ic_input_add" />


</androidx.constraintlayout.widget.ConstraintLayout>